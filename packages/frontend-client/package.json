{
  "name": "frontend-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ../../node_modules ./src -o ./src",
    "watch-css": "yarn build-css && node-sass-chokidar --include-path ./src --include-path ../../node_modules --watch ./src -o ./src",
    "start-js": "BROWSER=none react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "analyse": "source-map-explorer build/static/js/vendors.* && source-map-explorer build/static/js/main.*"
  },
  "dependencies": {
    "@material/chips": "0.38.1",
    "@material/list": "0.38.1",
    "@material/react-button": "0.4.2",
    "@material/react-card": "0.4.2",
    "@material/react-fab": "0.4.2",
    "@material/react-floating-label": "0.4.1",
    "@material/react-line-ripple": "0.5.0",
    "@material/react-notched-outline": "0.4.1",
    "@material/react-ripple": "0.4.2",
    "@material/react-text-field": "0.4.2",
    "@material/react-top-app-bar": "0.4.2",
    "@material/typography": "0.38.0",
    "apollo-boost": "0.1.14",
    "apollo-client": "2.4.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "react": "16.4.2",
    "react-apollo": "2.1.11",
    "react-dom": "16.4.2"
  },
  "devDependencies": {
    "node-sass-chokidar": "1.3.3",
    "npm-run-all": "4.1.3",
    "react-scripts": "2.0.0-next.3e165448",
    "source-map-explorer": "1.6.0"
  }
}
